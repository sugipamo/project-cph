      "local": {
        "run_prepare_file_moves": [
          {
            "src": "{contest_current_path}/{source_file_name}",
            "dst": "{workspace_path}/{source_file_name}"
          }
        ],
        "run_cmd": ["python3", "{workspace_path}/{source_file_name}"],
