{
  "rust": {
    "language_id": "5078",
    "source_file_name": "src/main.rs",
    "contest_current_path": "./contest_current",
    "exclude_patterns": [
      "^.*\\.log$",
      "^debug.*"
    ],
    "env_types": {
      "docker": {
        "dockerfile_path": "./contest_env/rust/Dockerfile",
        "run_prepare_file_moves": [],
        "run_cmd": ["cargo", "run"],
        "build_prepare_file_moves": [
          {
            "src": "{contest_current_path}/src",
            "dst": "{workspace_path}/src"
          }
        ],
        "build_cmds": [
          ["cargo", "build"]
        ]
      },
      "local": {
        "run_prepare_file_moves": [],
        "run_cmd": ["cargo", "run"],
        "build_prepare_file_moves": [
          {
            "src": "{contest_current_path}/src",
            "dst": "{workspace_path}/src"
          }
        ],
        "build_cmds": [
          ["cargo", "build"]
        ]
      }
    }
  }
}