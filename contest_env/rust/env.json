{
  "rust": {
    "aliases": ["rs"],
    "language_id": "5078",
    "source_file_name": "src/main.rs",
    "contest_current_path": "./contest_current",
    "contest_template_path": "./contest_template/{language_name}",
    "exclude_patterns": [
      "^.*\\.log$",
      "^debug.*"
    ],
    "commands": {
      "open": {
        "aliases": ["o"],
        "description": "コンテストを開く",
        "run": [
          {
            "type": "shell",
            "allow_failure": true,
            "show_output": true,
            "cmd": ["code", "{contest_current_path}/{source_file_name}"]
          }
        ]
      },
      "test": {
        "aliases": ["t"],
        "description": "テストを実行する",
        "run": [
          {
            "type": "test",
            "allow_failure": false,
            "show_output": true,
            "cmd": ["cargo", "test"]
          }
        ]
      },
      "submit": {
        "aliases": ["s"],
        "description": "提出する",
        "run": [
          {
            "type": "oj",
            "allow_failure": false,
            "show_output": true,
            "cmd": ["oj", "submit", "https://{contest_name}.contest.atcoder.jp/tasks/{contest_name}_{problem_id}", "{contest_current_path}/{source_file_name}"]
          }
        ]
      }
    },
    "env_types": {
      "docker": {
        "aliases": ["docker"],
        "dockerfile_path": "./contest_env/rust/Dockerfile",
        "run_prepare_file_moves": [],
        "run_cmd": ["cargo", "run"],
        "build_prepare_file_moves": [
          {
            "src": "{contest_current_path}/src",
            "dst": "{workspace_path}/src"
          }
        ],
        "build_cmds": [
          ["cargo", "build"]
        ]
      },
      "local": {
        "aliases": ["local"],
        "run_prepare_file_moves": [],
        "run_cmd": ["cargo", "run"],
        "build_prepare_file_moves": [
          {
            "src": "{contest_current_path}/src",
            "dst": "{workspace_path}/src"
          }
        ],
        "build_cmds": [
          ["cargo", "build"]
        ]
      }
    }
  }
}