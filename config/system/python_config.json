{
  "python_config": {
    "interpreters": {
      "default": "python3",
      "alternatives": ["python3.11", "python3.10", "python3.9", "python3.8", "python", "/usr/bin/python3"],
      "minimum_version": "3.8.0"
    },
    "execution": {
      "default_timeout": 30,
      "memory_limit": "256MB",
      "working_directory": ".",
      "encoding": "utf-8"
    },
    "packages": {
      "required": ["sys", "os", "subprocess"],
      "optional": ["numpy", "scipy"],
      "check_availability": true
    },
    "error_handling": {
      "interpreter_not_found": "error",
      "version_mismatch": "warning",
      "package_missing": "warning"
    }
  }
}